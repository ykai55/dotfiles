[user]
	name = ykai
	email = me@ykai.cc
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[color]
	ui = always
	status = always
[core]
	autocrlf = input
[push]
	autoSetupRemote = true
[alias]
  diff = diff --color
	cp = cherry-pick
	st = status
	br = branch
	cm = commit
  ck = checkout
	last = log -1 HEAD --format=%H
  root = rev-parse --show-toplevel
  skip = update-index --skip-worktree
